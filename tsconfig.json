{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "eyc-regulatory-reporting": [
        "dist/eyc-regulatory-reporting/eyc-regulatory-reporting",
        "dist/eyc-regulatory-reporting"
      ],
      "@default/*": [
        "src/app/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "eyc-ui-shared-component": [
        "dist/eyc-ui-shared-component/eyc-ui-shared-component",
        "dist/eyc-ui-shared-component",
        "projects/eyc-ui-shared-component/src/lib*"
      ],
      "eyc-data-intake": [
        "dist/eyc-data-intake/eyc-data-intake",
        "dist/eyc-data-intake"
      ],
      "eyc-tax-reporting": [
        "dist/eyc-tax-reporting/eyc-tax-reporting",
        "dist/eyc-tax-reporting"
      ],
      "eyc-data-managed-services": [
        "dist/eyc-data-managed-services/eyc-data-managed-services",
        "dist/eyc-data-managed-services"
      ],
      "eyc-charts-shared-library": [
        "dist/eyc-charts-shared-library/eyc-charts-shared-library",
        "dist/eyc-charts-shared-library",
        "project/eyc-charts/src/public-api"
      ],
      "eyc-charts-shared-library/*": [
        "projects/eyc-charts-shared-library/src/lib/*"
      ],
      "eyc-admin": [
        "dist/eyc-admin/eyc-admin",
        "dist/eyc-admin",
        "projects/eyc-admin/src/lib*"
      ],
      "eyc-european-fund-reporting": [
        "dist/eyc-european-fund-reporting/eyc-european-fund-reporting",
        "dist/eyc-european-fund-reporting"
      ]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*"
  ],
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}